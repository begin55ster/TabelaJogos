<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">


	<navigation-rule>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/paginas/login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/paginas/index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>admin</from-outcome>
			<to-view-id>/paginas/protected/admin/admin.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>navegar-CadastroTime</from-outcome>
			<to-view-id>/paginas/CadastroTime.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>navegar-CadastroJogador</from-outcome>
			<to-view-id>/paginas/CadastroJogador.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>navegar-Configuracoes</from-outcome>
			<to-view-id>/paginas/Configuracoes.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>navegar-voltarConfiguracoes</from-outcome>
			<to-view-id>/paginas/index.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>navegar-CadastroTimehome</from-outcome>
			<to-view-id>/paginas/index.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>navegar-CadastroJogadorHome</from-outcome>
			<to-view-id>/paginas/index.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>navegar-MontarCampeonato</from-outcome>
			<to-view-id>/paginas/MontarCampeonato.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>navegar-pontosCorridos</from-outcome>
			<to-view-id>/paginas/pontosCorridos.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	
	

</faces-config>
